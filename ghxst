        wait(10)
        game.ReplicatedStorage.Package.Events.Start:InvokeServer()
        game.ReplicatedStorage.Package.Events.Start:InvokeServer()
        game.ReplicatedStorage.Package.Events.Start:InvokeServer()
        game.Players.LocalPlayer.Character.Humanoid.Health = 0
    
    wait(5)
     local A_1 = "Godly SSJ2"
    local Event = game:GetService("ReplicatedStorage").Package.Events.equipskill
