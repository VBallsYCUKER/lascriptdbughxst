local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("ScriptHub GHXST","DarkTheme")

local CoreGui = game:GetService("StarterGui")

local Tab = Window:NewTab("FARMS")
local Section = Tab:NewSection("Farmstuff")

Section:NewButton("AntiAfk", "AntiAfk", function ()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/luca5432/Roblox-ANTI-AFK-SCRIPT/main/Script", true))()
end)

local Tab = Window:NewTab("Forms")
local Section = Tab:NewSection("Forms and Stack")

Section:NewButton("Stack Godly SSJ2 (wait 10 seconds)", "After you press wait 10 seconds", function()

local Forms = {'Blanco', 'Ultra Ego', 'SSJR3', 'SSJR3', 'SSJB3',
